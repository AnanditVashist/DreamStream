@model IEnumerable<DreamStream.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>
@Html.ActionLink("Add Movie","New","",new { @class="btn btn-primary"})
@foreach (var movie in Model)
{
<ul>
    <li>@Html.ActionLink(movie.Name,"Edit",new { id = movie.Id })-@movie.Genre.Type</li>
</ul>
}
