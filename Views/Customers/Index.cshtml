@model System.Collections.Generic.List<DreamStream.Models.Customer>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customer</h2>
@foreach (var customer in Model)
{
    <ul>
        <li>@Html.ActionLink(customer.Name,"edit","Customers",null, new{ id=@customer.Id })</li>
    </ul>
}